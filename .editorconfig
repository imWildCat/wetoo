# EditorConfig is awesome: http://EditorConfig.org

root = true

[*]
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[*.js]
indent_style = space
indent_size = 2

[*.ts]
indent_style = tab
indent_size = 4

[{package.json,.travis.yml}]
indent_style = space
indent_size = 2
